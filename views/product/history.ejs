<%- include('../includes/head.ejs') %>
<body>
    

    <%- include('../includes/navigation.ejs') %>
    <div class="main-content">
        <div class="section-content section-content-p30">
            <div class="container-fluid">
                <a href="/customer-prds" class="btn btn-info">Back To Products</a>
                <div class="row">
                    <% for(let products of historyArr){ %>
                    <div class="col-md-2">
                        <div class="product-box">
                            <img src="<%=products.image%>" class="img-responsive" alt="image not found">
                            <h5  style="background-color:#48D1CC;">Type:<%=products.name%></h3>
                            <h5 style="color:#FF7F50;">Current Price: <span>&#36</span> <%=products.newPrice%> </h5>
                            <h5 style="color:crimson ;">Old Price: <span>&#36</span> <%=products.oldPrice%> </h5>
                            <h5 style="color:#6495ED;" > Quantity : <%=products.quantity%> </h5>
                            <h5 style="color:#663399;" >Date :<%=products.date%></h5>
        
                        </div>
                    </div>
                    <% }%>
                </div>
            </div>
        </div>
    </div>
    <%- include('../includes/footer')%>
</body>

</html>