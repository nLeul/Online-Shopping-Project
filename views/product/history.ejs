<%- include('../includes/head.ejs') %>
<body>

    <%- include('../includes/navigation.ejs') %>
    <div class="main-content">
        <div class="section-content section-content-p30">
            <div class="container-fluid">
                <a href="/customer-prds" class="btn btn-info">Back To Products</a>
                <div class="row">
                    <% for(let products of historyArr){ %>
                    <div class="col-md-2">
                        <div class="product-box">
                            <img src="<%=products.image%>" class="img-responsive" alt="image not found">
                            <h1>Name: <%=products.name%></h1>
                            <h1>Current Price: <span>&#36</span> <%=products.newPrice%> </h1>
                            <h1>Old Price: <span>&#36</span> <%=products.oldPrice%> </h1>
                            <div class="price"> Quantity : <%=products.quantity%> </div>
                            <div class="price">Date :<%=products.date%></div>
        
                        </div>
                    </div>
                    <% }%>
                </div>
            </div>
        </div>
    </div>
    <%- include('../includes/footer')%>
</body>

</html>